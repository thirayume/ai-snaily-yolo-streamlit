# Auto detect text files and perform LF normalization
* text=auto
yolo_versions/*.weights filter=lfs diff=lfs merge=lfs -text
yolo_versions/*.pt filter=lfs diff=lfs merge=lfs -text
*.pt filter=lfs diff=lfs merge=lfs -text
yolo_versions/*.h5 filter=lfs diff=lfs merge=lfs -text
yolo_versions/*.onnx filter=lfs diff=lfs merge=lfs -text
